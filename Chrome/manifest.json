{
	"name": "Dollchan Extension Tools",
	"version": "15.1.18.2",
	"description": "The best way to browse imageboards",
	"homepage_url": "https://github.com/SthephanShinkufag/Dollchan-Extension-Tools/",
	"content_scripts": [ {
		"all_frames": true,
		"js": [ "Dollchan_Extension_Tools.user.js" ],
		"matches": [ "*://*/*" ],
		"run_at": "document_start"
	} ],
	"icons": {
		"16": "16.png",
		"32": "32.png",
		"48": "48.png",
		"128": "128.png"
	},
	"permissions": [
		"storage",
		"unlimitedStorage",
		"notifications"
	],
	"manifest_version": 2
}
